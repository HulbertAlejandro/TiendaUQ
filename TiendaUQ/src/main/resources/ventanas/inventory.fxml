<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="414.0" prefWidth="648.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.tiendaUQ.controladores.InventoryControlador">
   <children>
      <Label alignment="CENTER" layoutX="47.0" layoutY="22.0" prefHeight="33.0" prefWidth="555.0" text="INVENTARIO" textAlignment="CENTER" textFill="#0069ff" textOverrun="CENTER_ELLIPSIS">
         <font>
            <Font name="Inter Semi Bold" size="24.0" />
         </font>
      </Label>
      <TableView layoutX="24.0" layoutY="83.0" prefHeight="257.0" prefWidth="320.0">
        <columns>
          <TableColumn prefWidth="63.0" text="Code" />
          <TableColumn prefWidth="94.0" text="Product name" />
            <TableColumn prefWidth="73.0" text="Price" />
            <TableColumn prefWidth="89.0" text="Quantity" />
        </columns>
      </TableView>
      <Button fx:id="btnSeleccionar" layoutX="41.0" layoutY="352.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="165.0" text="SELECT PRODUCT">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font>
      </Button>

      <Button fx:id="btnEliminar" layoutX="215.0" layoutY="352.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="101.0" text="REMOVE">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font>
      </Button>
       <TextField fx:id="txtCodigo" layoutX="401.0" layoutY="96.0" prefHeight="25.0" prefWidth="200.0" promptText="Code">
           <font>
               <Font name="Calibri Bold" size="12.0" />
           </font></TextField>
       <TextField fx:id="txtNombre" layoutX="401.0" layoutY="138.0" prefHeight="25.0" prefWidth="200.0" promptText="Product name">
           <font>
               <Font name="Calibri Bold" size="12.0" />
           </font></TextField>
       <TextField fx:id="txtPrecio" layoutX="401.0" layoutY="175.0" prefHeight="25.0" prefWidth="200.0" promptText="Price">
           <font>
               <Font name="Calibri Bold" size="12.0" />
           </font></TextField>
       <TextField fx:id="txtCantidad" layoutX="401.0" layoutY="218.0" prefHeight="25.0" prefWidth="200.0" promptText="Quantity">
           <font>
               <Font name="Calibri Bold" size="12.0" />
           </font></TextField>
      <Button fx:id="btnEditar" layoutX="480.0" layoutY="259.0" mnemonicParsing="false" text="EDIT">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font>
      </Button>
       <Button fx:id="btnRegresar" layoutX="477.0" layoutY="294.0" mnemonicParsing="false" onAction="#regresarLogin" text="BACK">
           <font>
               <Font name="Calibri Bold" size="13.0" />
           </font>
       </Button>
   </children>
</AnchorPane>
